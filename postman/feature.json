	"item": [
		{
			"name": "Subscribe",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"const main = async () => {",
							"    const api = new API(pm);",
							"    const rnd = new RandomUtils();",
							"",
							"    try {",
							"        const user1 = await api.addUser(rnd.getUser());",
							"        pm.collectionVariables.set(\"uid\", user1.id)",
							"        const user2 = await api.addUser(rnd.getUser());",
							"        pm.collectionVariables.set(\"uid1\", user2.id)",
							"    } catch(err) {",
							"        console.error(\"Ошибка при подготовке тестовых данных.\", err);",
							"    }",
							"};",
							"",
							"const interval = setInterval(() => {}, 1000);",
							"",
							"setTimeout(async () => ",
							"    {",
							"        try {",
							"            await main();",
							"        } catch (e) {",
							"            console.error(e);",
							"        } finally {",
							"            clearInterval(interval);",
							"        }",
							"    },  ",
							"    100 ",
							");"
						],
						"type": "text/javascript",
						"packages": {}
					}
				},
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Ответ должен содержать код статуса 201 и данные в формате json\", function () {",
							"    pm.response.to.have.status(201);   ",
							"    pm.response.to.be.withBody;",
							"    pm.response.to.be.json;",
							"});",
							"",
							"const target = pm.response.json();",
							"",
							"pm.test(\"Подписка должна содержать поля: id, follower, followed\", function () {",
							"pm.expect(target).to.have.property('id');",
							"pm.expect(target).to.have.property('follower');",
							"pm.expect(target).to.have.property('followed');",
							"});"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
