		{
			"name": "Feed",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"const main = async () => {",
							"    const api = new API(pm);",
							"    const rnd = new RandomUtils();",
							"",
							"    try {",
							"        const user1 = await api.addUser(rnd.getUser());",
							"        pm.collectionVariables.set(\"uid\", user1.id)",
							"        const user2 = await api.addUser(rnd.getUser());",
							"        const user3 = await api.addUser(rnd.getUser());",
							"        await api.addSubscription(user1.id, user2.id)",
							"        const category = await api.addCategory(rnd.getCategory());",
							"        let eventBody = rnd.getEvent(category.id);",
							"        eventBody['requestModeration'] = true;",
							"        eventBody['participantLimit'] =  0;",
							"        let event = await api.addEvent(user3.id, eventBody);",
							"        event = await api.publishEvent(event.id);",
							"        let request = await api.publishParticipationRequest(event.id, user2.id);",
							"        request = await api.acceptParticipationRequest(event.id, user3.id, request.id);",
							"    } catch(err) {",
							"        console.error(\"Ошибка при подготовке тестовых данных.\", err);",
							"    }",
							"};",
							"",
							"const interval = setInterval(() => {}, 1000);",
							"",
							"setTimeout(async () => ",
							"    {",
							"        try {",
							"            await main();",
							"        } catch (e) {",
							"            console.error(e);",
							"        } finally {",
							"            clearInterval(interval);",
							"        }",
							"    },  ",
							"    100 ",
							");"
						],
						"type": "text/javascript",
						"packages": {}
					}
				},
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Ответ должен содержать код статуса 200 и данные в формате json\", function () {",
							"    pm.response.to.be.ok;    ",
							"    pm.response.to.be.withBody;",
							"    pm.response.to.be.json;",
							"});",
							"",
							"const source = pm.collectionVariables.get('response');",
							"const target = pm.response.json();",
							"",
							"pm.test(\"Информация об участии должна содержать поля: userId, userName, eventId, eventTitle, eventDate\", function () {",
							"pm.expect(target[0]).to.have.property('userId');",
							"pm.expect(target[0]).to.have.property('userName');",
							"pm.expect(target[0]).to.have.property('eventId');",
							"pm.expect(target[0]).to.have.property('eventTitle');",
							"pm.expect(target[0]).to.have.property('eventDate');",
							"});",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
